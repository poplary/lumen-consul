{
    "name": "poplary/lumen-consul",
    "description": "Consul for Lumen",
    "keywords": ["consul", "laravel", "lumen"],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/log": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "poplary/consul-sdk": "^0.1",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Poplary\\LumenConsul\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
